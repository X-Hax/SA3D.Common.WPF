<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="../SA3D.ProjectConfigurations/NuGet.Build.props" />

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<RootNamespace>SA3D.Common.WPF</RootNamespace>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<PropertyGroup>
		<Title>Reused WPF code for SA3D applications</Title>
		<Product>Reused WPF code for SA3D applications</Product>
		<Description>Reused WPF code for SA3D applications.</Description>
		<Version>1.0.0</Version>
		<RepositoryUrl>https://github.com/X-Hax/SA3D.Common.WPF</RepositoryUrl>
		<PackageProjectUrl>https://github.com/X-Hax/SA3D.Common.WPF</PackageProjectUrl>
		<PackageId>SA3D.Common.WPF</PackageId>
		<PackageReadmeFile>README.md</PackageReadmeFile>
