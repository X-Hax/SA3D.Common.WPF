	<ItemGroup>
		<None Include="../../README.md" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Gu.Wpf.NumericInput" Version="0.5.5" />
	</ItemGroup>

</Project>
